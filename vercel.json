{
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/templates/(.*)",
            "dest": "templates/$1"
        },
        {
            "src": "/assets/(.*)",
            "dest": "assets/$1"
        },
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}